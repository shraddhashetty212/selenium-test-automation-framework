package com.wrapper;

import java.util.ArrayList;

public class ArrayListDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ArrayList al = new ArrayList();
		
		//add items to array list
		al.add(10); //here integer 10 will be added as an object
		al.add("Shraddha");
		al.add(true);
		al.add("Shraddha");  //array list can have duplicate values
		al.add(true);
		al.add("Shetty");  //array list can have duplicate values
		al.add(false);
		System.out.println(al.size());
		System.out.println(al);
		
		//remove items from array list
		al.remove(0);
		System.out.println(al);
		
		//Fetch
		System.out.println(al.get(0));
		System.out.println(al.get(1));
		
		//store
		for (int i = 0; i < al.size(); i++)
		{
			System.out.println(al.get(i));
		}

	}

}

/*
 * https://www.javadoc.io/doc/com.github.gwtd3/gwt-d3-api/latest/com/github/gwtd3/api/arrays/ArrayList.html
 */
