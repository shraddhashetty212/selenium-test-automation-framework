package learningjava;

public class MultiDimensionalArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[][] myMultiDimArray = new int[2][3];
		myMultiDimArray[0][0] = 10;
		myMultiDimArray[0][1] = 20;
		myMultiDimArray[0][2] = 30;
		myMultiDimArray[1][0] = 40;
		myMultiDimArray[1][1] = 50;
		myMultiDimArray[1][2] = 60;
		
		System.out.println(myMultiDimArray[1][0]);

	}

}
