package learningjava;

public class NestedLoopsDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i = 1; i <= 4; i++)
		{
			int j = 1;
			while (j <= 5)
			{
				System.out.println("Value of i is "+i+" and value of j is "+j);
				j++;
			}
		}

	}

}
